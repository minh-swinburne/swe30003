{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DbSettings": {
    "Provider": "MySql | SqlServer | PostgreSql",
    "ConnectionString": "Server=<DB_SERVER>;Database=<DB_DATABASE>;User Id=<DB_USER>;Password=<DB_PASSWORD>;",
    "UseSnakeCaseNaming": true
  },
  "JwtSettings": {
    "Issuer": "<JWT_ISSUER>",
    "Audience": "<JWT_AUDIENCE>",
    "Algorithm": "<JWT_ALGORITHM>",
    "SecretKey": "<JWT_SECRET_KEY>",
    "ExpirationInMinutes": 60
  },
  "MapSettings": {
    "GoogleApiKey": "<GOOGLE_MAPS_API_KEY>"
  },
  "TransactionSettings": {
    "TransactionFee": 0.05,
    "MinimumTransactionAmount": 1.00,
    "MaximumTransactionAmount": 1000.00,
    "DefaultCurrency": "USD",
    "CreditCard": {
      "CardNumber": "<YOUR_CREDIT_CARD_NUMBER>",
      "ExpirationDate": "<EXPIRATION_DATE>",
      "Cvc": "<YOUR_CREDIT_CARD_CVC>",
      "CardHolderName": "<YOUR_NAME_ON_CARD>"
    },
    "PayPal": {
      "ClientId": "<YOUR_PAYPAL_CLIENT_ID>",
      "ClientSecret": "<YOUR_PAYPAL_CLIENT_SECRET>",
      "SandboxMode": true
    }
  }
}
